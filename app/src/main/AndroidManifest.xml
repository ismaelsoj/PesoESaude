<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.app.ismael.pesoesaude">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CadastroUserActivity"
            android:label="@string/activity_cadastro_name"></activity>
        <activity
            android:name=".PesoActivity"
            android:label="@string/activity_peso_name"></activity>
        <activity android:name=".BoasVindasActivity"></activity>
        <activity
            android:name=".ListaPesosActivity"
            android:label="@string/activity_lista_peso_name"></activity>
    </application>

</manifest>
